<div class="sidebar">
  <div class="sidebar-top">
    <img class="app-logo" src="../img/AppLogo.png" />
    <p class="app-name">NOVA</p>
  </div>
  <div class="sidebar-content">
    <button id="share-workspace">Share</button>
  </div>
</div>
<div class="board">
  <form id="todo-form">
    <input type="text" placeholder="New TODO..." id="todo-input" />
    <button type="submit">Add +</button>
  </form>

  <div class="lanes">
    <div class="swim-lane" id="todo-lane">
      <h3 class="heading">TODO</h3>
    </div>

    <div class="swim-lane" id="progress-lane">
      <h3 class="heading">In progress</h3>
    </div>

    <div class="swim-lane" id="done-lane">
      <h3 class="heading">Done</h3>
    </div>
  </div>
</div>

{{> issueCreatePopUp}}
{{> issueEditPopUp}}
{{> sharePopUp}}

<div id="overlay"></div>
<script>
  const workspaceID = "{{workspaceID}}";
</script>
