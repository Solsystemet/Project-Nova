<main>
  {{> navbar}}
  <form action="/login" method="POST">
    <h1>Welcome back</h1>
    <input
      id="username"
      name="username"
      placeholder="Username"
      type="text"
      required
    />
    <input
      id="password"
      name="password"
      placeholder="Password"
      type="password"
      required
    />

    <button>Login</button>
  </form>
</main>
